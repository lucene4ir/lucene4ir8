<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<tokenFilters>
    <tokenizer>standard</tokenizer>
    <tokenFilter>
        <name>lowercase</name>
    </tokenFilter>
    <tokenFilter>
        <name>stop</name>
    </tokenFilter>
    <tokenFilter>
        <name>patternreplace</name>
        <param>
            <key>pattern</key>
            <value>([+\-._(),/%]?[0-9]+[+\-._(),/%]?[0-9]*[+\-._(),/%]?|^_+)</value>
        </param>
        <param>
            <key>replacement</key>
            <value></value>
        </param>
        <param>
            <key>replace</key>
            <value>all</value>
        </param>
    </tokenFilter>
    <tokenFilter>
        <name>porterstem</name>
    </tokenFilter>
    <tokenFilter>
        <name>length</name>
        <param>
            <key>min</key>
            <value>3</value>
        </param>
        <param>
            <key>max</key>
            <value>999</value>
        </param>
    </tokenFilter>
    <tokenFilter>
        <name>shingle</name>
        <param>
            <key>minShingleSize</key>
            <value>2</value>
        </param>
        <param>
            <key>maxShingleSize</key>
            <value>2</value>
        </param>

        <param>
            <key>fillerToken</key>
            <value>___</value>
        </param>
        <param>
            <key>outputUnigrams</key>
            <value>False</value>
        </param>
    </tokenFilter>
</tokenFilters>