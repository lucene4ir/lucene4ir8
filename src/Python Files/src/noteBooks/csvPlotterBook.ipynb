     "    return result\n",
     "\n",
     "\n",
     "def showFigure(figNum):\n",
     "    global GModel, GCorpus, GXAxis, GYAxis, GFBTerms, GPlotType, GCoefficient, GCsvOutPath, \\\n",
     "        GIndexStructureType, GSpecificTermType, GPredefinedTicks\n",
     "\n",
     "    # Main Title\n",
     "    model = getModel(GModel)\n",
