     "    elif GModel in allModels and index == modelCoefficientIndex:\n",
     "        header = GModel.lower()\n",
     "    elif index == fbTermsIndex:\n",
     "        header = 'fbterms'\n",
     "    elif plotTypeGroup != GIndexStructureType:\n",
     "        header = 'rm3Map'\n",
     "    else:\n",
     "        switcher = {\n",
     "            cwlMapIndex: 'cwlMap',\n",
     "            cwlP10Index: 'cwlP10',\n",
     "            rbp6Index: 'r6',\n",
