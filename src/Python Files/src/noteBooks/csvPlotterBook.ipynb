     "        print('File %s is Saved' % fileName)\n",
     "\n",
     "\n",
     "def getLimits(limits, values):\n",
     "    minVal = min(values)\n",
     "    maxVal = max(values)\n",
     "\n",
     "    if limits[0] > minVal:\n",
     "        limits[0] = minVal\n",
     "    if limits[1] < maxVal:\n",
     "        limits[1] = maxVal\n",
     "    return limits\n",
     "\n",
     "\n",
     "def resetLimits():\n",
     "    global GXLimits, GYLimits\n",
